:boot
imgfree
kernel ${kernel_url}vmlinuz misobasedir=garuda ${ipparam} miso_http_srv=${fetch_url} nouveau.modeset=1 i915.modeset=1 radeon.modeset=1 driver=free initrd=initrd.magic ${cmdline}
initrd ${kernel_url}initramfs.img
boot

:live_exit
clear menu
exit 0
