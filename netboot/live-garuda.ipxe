#!ipxe

# Garuda Linux
# https://garudalinux.org/

goto ${menu} ||

:live-garuda
set os Garuda
menu ${os} Live
set ipparam BOOTIF=${netX/mac} ip=dhcp net.ifnames=0
item --gap ${os} Live versions
item 250801-cinnamon ${space} ${os} Cinnamon (250801)
item 250801-dr460nized ${space} ${os} Dr460nized (250801)
item 250801-dr460nized-gaming ${space} ${os} Dr460nized-Gaming (250801)
item 250801-gnome ${space} ${os} Gnome (250801)
item 250801-i3wm ${space} ${os} I3wm (250801)
item 220329-kde-barebones ${space} ${os} Kde-Barebones (220329)
item 230501-lxqt-kwin ${space} ${os} Lxqt-Kwin (230501)
item 230501-mate ${space} ${os} Mate (230501)
item 231029-qtile ${space} ${os} Qtile (231029)
item 250801-sway ${space} ${os} Sway (250801)
item 230501-wayfire ${space} ${os} Wayfire (230501)
item 250801-xfce ${space} ${os} Xfce (250801)
choose menu || goto live_exit
goto ${menu}

:250801-cinnamon
set fetch_url ${live_endpoint}/asset-mirror/releases/download/250801-0b360492/
set kernel_url ${live_endpoint}/asset-mirror/releases/download/250801-0b360492/
goto boot

